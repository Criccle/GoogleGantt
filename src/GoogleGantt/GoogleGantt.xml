<?xml version="1.0" encoding="utf-8" ?>
<widget id="GoogleGantt.widget.GoogleGantt" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>GoogleGantt</name>
    <description>Implement Google Gantt in your project!</description>

    <icon>
    
    </icon>

    <properties>
        <property key="rowItem" type="entity" required="false" isPath="no" allowNonPersistableEntities="true">
            <caption>Row entity</caption>
            <category>Behaviour</category>
            <description>The row entity. This entity must be returned by the list with rows microflow</description>        
        </property>
        <property key="rowList" type="microflow" required="true">
            <caption>List with rows</caption>
            <category>Behaviour</category>
            <description>Microflow which returns a list of rows with the chart data</description>
            <returnType type="Object" isList="true" entityProperty="rowItem"/>
        </property>
        <property key="criticalPathEnabled" type="boolean" defaultValue="true">
            <caption>CriticalPath enabled</caption>
            <category>Critical Path</category>
            <description>Set to true to enable display of the critical path</description>
        </property>
        <property key="criticalPathStyleStroke" type="string" required="false">
            <caption>CriticalPath stroke style</caption>
            <category>Critical Path</category>
            <description>The color of any critical path arrows</description>
        </property>               
        <property key="criticalPathStyleStrokeWidth" type="string" defaultValue="1.4">
            <caption>CriticalPath stroke width</caption>
            <category>Critical Path</category>
            <description>The thickness of any critical path arrows</description>
        </property>     
        <property key="ganttArrowColor" type="string" defaultValue="#000">
            <caption>Color of the arrows</caption>
            <category>Arrows</category>
            <description>The color of the arrows</description>
        </property>        
        <property key="ganttArrowWidth" type="string" defaultValue="1.4">
            <caption>Width of the arrows</caption>
            <category>Arrows</category>
            <description>The thickness of the arrows</description>
        </property>     
        <property key="ganttArrowAngle" type="integer" defaultValue="45">
            <caption>Angle of the head of the arrows</caption>
            <category>Arrows</category>
            <description>The angle of the head of the arrows</description>
        </property>     
        <property key="ganttArrowRadius" type="integer" defaultValue="15">
            <caption>Radius of the arrows</caption>
            <category>Arrows</category>
            <description>The radius for defining the curve of the arrow between two tasks</description>
        </property>     
        <property key="ganttGridLineStroke" type="string" required="false">
            <caption>Color of the grid lines</caption>
            <category>Grids</category>
            <description>The color of the inner horizontal grid lines</description>
        </property>        
        <property key="ganttGridLineStrokeWidth" type="integer" defaultValue="1">
            <caption>Width of the grid lines</caption>
            <category>Grids</category>
            <description>The thickness of the inner horizontal grid lines</description>
        </property>   
        <property key="ganttGridTrackFill" type="string" required="false">
            <caption>Color of the grid track</caption>
            <category>Grids</category>
            <description>The fill color of the inner grid track</description>
        </property>  
        <property key="ganttGridDarkTrackFill" type="string" required="false">
            <caption>Color of the dark grid track</caption>
            <category>Grids</category>
            <description>The fill color of the inner dark grid track</description>
        </property>  
        <property key="ganttLabelStyleFont" type="string" defaultValue="Arial">
            <caption>Label font</caption>
            <category>Labels</category>
            <description>The label font</description>
        </property>  
        <property key="ganttLabelStyleFontSize" type="integer" defaultValue="12">
            <caption>Label font size</caption>
            <category>Labels</category>
            <description>The label font size</description>
        </property>  
        <property key="ganttLabelStyleColor" type="string" defaultValue="#757575">
            <caption>Label font color</caption>
            <category>Labels</category>
            <description>The label font color</description>
        </property>  
    </properties>
</widget>

